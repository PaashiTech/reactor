{
  "name": "@unmaze/expo-app",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "dev": "expo prebuild && expo start --dev-client",
    "clean": "if [ -d \"./dist\" ]; then rm -R ./dist; fi",
    "build": "expo prebuild",
    "lint": "eslint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "android": "expo run:android",
    "android-build-clean": "cd android && .\\gradlew.bat clean",
    "ios": "expo run:ios",
    "eas-build": "eas build --profile development --platform android",
    "eas-build-pre-install": "npm i -g pnpm@8.15.0"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@unmaze/assets": "workspace:*",
    "@unmaze/mobile": "workspace:*",
    "expo": "~49.0.18",
    "expo-dev-client": "^2.4.12",
    "expo-linking": "~5.0.2",
    "expo-splash-screen": "~0.20.5",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.10",
    "react-native-drop-shadow": "^1.0.0",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-gradle-plugin": "^0.71.19",
    "react-native-material-datetime-picker": "^0.2.1",
    "react-native-mmkv": "^2.12.1",
    "react-native-reanimated": "^3.7.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0",
    "react-native-svg": "13.9.0",
    "react-native-svg-transformer": "^1.3.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-webview": "13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-private-methods": "^7.23.3"
  }
}
