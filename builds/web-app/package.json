{
  "name": "@unmaze/web-app",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "start": "dev:vite",
    "web": "yarn dev",
    "clean": "echo \"Error: no clean script\" && exit 1",
    "lint": "eslint",
    "dev:vite": "cross-env NODE_ENV=development vite --host --clearScreen false",
    "dev:vite:extract": "cross-env EXTRACT=1 yarn dev",
    "dev": "dev:vite",
    "dev:webpack": "cross-env DISABLE_EXTRACTION=true NODE_ENV=development webpack serve",
    "dev:extract": "cross-env DISABLE_EXTRACTION=false NODE_ENV=development webpack serve",
    "deploy": "cross-env NODE_ENV=production webpack --json=dist/compilation-stats.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@tamagui/animations-css": "^1.85.12",
    "@tamagui/config": "^1.85.12",
    "@tamagui/core": "^1.85.12",
    "@tamagui/create-theme": "^1.85.12",
    "@tamagui/react-native-media-driver": "^1.85.12",
    "@tamagui/react-native-svg": "^1.85.12",
    "@unmaze/web": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native-web": "~0.19.9",
    "tamagui": "^1.85.12"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.11",
    "@tamagui/vite-plugin": "^1.85.12",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "async-retry": "1.3.1",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "esbuild-loader": "^4.0.2",
    "esbuild-register": "^3.4.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "react-refresh": "^0.14.0",
    "style-loader": "^3.3.1",
    "tamagui-loader": "^1.85.12",
    "thread-loader": "^3.0.4",
    "url-loader": "^4.1.1",
    "vite": "^5.0.2",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
